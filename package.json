{
  "name": "plexacious",
  "version": "1.0.0",
  "description": "A library to facilitate talking to a Plex server via its web API.",
  "main": "Plexacious.js",
  "scripts": {
    "build": "node node_modules/gulp/bin/gulp babelify",
    "build:watch": "node node_modules/gulp/bin/gulp",
    "configure": "node lib/configure",
    "postinstall": "npm run build",
    "pretest": "npm run build",
    "test": "node node_modules/mocha/bin/mocha --compilers js:babel-register",
    "test:watch": "node node_modules/mocha/bin/mocha --compilers js:babel-register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ketsugi/plexacious.git"
  },
  "keywords": [
    "plex",
    "api"
  ],
  "author": "Joel Pan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ketsugi/plexacious/issues"
  },
  "homepage": "https://github.com/ketsugi/plexacious",
  "dependencies": {
    "inquirer": "^1.0.3",
    "is-integerish": "^1.0.1",
    "jsonfile": "^2.3.1",
    "moment": "^2.13.0",
    "plex-api": "^3.5.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^2.5.3",
    "mocha-clean": "^1.0.0",
    "node-env-file": "^0.1.8"
  }
}
